{
  "name": "beers",
  "version": "1.0.0-alpha.0",
  "description": "A beer rating application",
  "main": "server.js",
  "engines": {
    "node": "^6.3.0",
    "npm": "^3.10.3"
  },
  "scripts": {
    "postinstall": "mkdirp dist cdn",
    "minify": "uglifyjs ./dist/build.js -o ./dist/build.js -m -c sequences,dead_code,conditionals,booleans,unused,if_return,join_vars,drop_console",
    "build:vue": "browserify -e src/index.js > ./dist/build.js",
    "build": "npm run build:vue && npm run minify && npm run revision",
    "heroku-prebuild": "npm run build",
    "watchify": "cross-env NODE_PATH=./node_modules watchify -v -d src/index.js -o ./dist/build.js",
    "serve": "node server.js",
    "start": "npm run serve",
    "livereload": "live-reload --port 9091 ./dist/",
    "test": "npm run test:lint",
    "test:prepare": "",
    "test:lint": "eslint \"src/**/**.js\" \"src/**/**.vue\" \"server/**/**.js\" \"server/**/**.vue\" \"server.js\"",
    "test:karma": "karma start"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "gwi-theme-common": "^1.0.1",
    "gwi-theming": "^1.0.0",
    "jade": "^1.11.0",
    "less": "^2.7.1",
    "pug": "^2.0.0-beta6",
    "svgshelf": "^1.0.3",
    "throng": "^4.0.0",
    "vue": "^2.0.2",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.0",
    "vuex": "^2.0.0",
    "vuex-router-sync": "^3.0.0"
  },
  "aliasify": {
    "aliases": {
      "vue": "vue/dist/vue"
    }
  },
  "devDependencies": {
    "aliasify": "^2.0.0",
    "babel-core": "^6.9.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.5",
    "envify": "^3.4.1",
    "hashmark": "^4.1.0",
    "live-reload": "^1.1.0",
    "mkdirp": "^0.5.1",
    "parallelshell": "^2.0.0",
    "replaceinfiles": "^1.1.4",
    "vueify": "^8.5.1",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "aliasify",
      "vueify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "brfs"
    ]
  }
}
